# Development Environment Configuration Example
# Copy this to .env for local development

ENVIRONMENT=development
DEBUG=true

# Database Configuration (Development)
DATABASE_URL=postgresql://user:password@db:5432/chatgpt_products
DB_SCHEMA=public
DB_TABLE=chatgpt_products

# CORS Configuration (Development)
ALLOWED_ORIGINS=http://localhost:8000,http://127.0.0.1:8000,https://chat.openai.com,https://chatgpt.com

# Security Configuration (Development)
SECRET_KEY=dev-secret-key-change-in-production

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Logging Configuration
LOG_LEVEL=DEBUG
