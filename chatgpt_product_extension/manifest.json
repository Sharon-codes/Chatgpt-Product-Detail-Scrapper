{
  "manifest_version": 3,
  "name": "ChatGPT Product Extractor",
  "version": "1.1",
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*",
        "https://aunpbdtpdp.us-west-2.awsapprunner.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*",
        "https://aunpbdtpdp.us-west-2.awsapprunner.com/*"
      ]
    }
  ],
  "host_permissions": ["https://aunpbdtpdp.us-west-2.awsapprunner.com/*"]
}
  